<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
<!--    <data>-->

<!--        <variable-->
<!--            name="value"-->
<!--            type="com.example.collegecounselor.WonFragment" />-->
<!--    </data>-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".WonFragment">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="115dp"
            android:text="@string/won"
            android:textSize="82sp" />

        <Button
            android:id="@+id/butscore"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/path"
            android:visibility="gone"
            android:textSize="25dp" />

        <Button
            android:layout_marginTop="90dp"
            android:id="@+id/play_again"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/facebookb"
            android:text="Play Again"
            android:textSize="25dp" />

        <Button
            android:text="Logout"
            android:layout_marginTop="90dp"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Logout_but"
            android:background="@drawable/googleb"
            />


    </LinearLayout>

</layout>